{
  "name": "react-native-accessibility-props",
  "version": "0.0.1",
  "description": "Get cross platform accessibily props for react-native and react-native-web",
  "module": "index.js",
  "main": "cjs/index.js",
  "scripts": {
    "test": "jest --coverage --verbose --passWithNoTests && tsd && npm run lint",
    "jest": "jest --verbose --coverage --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "npm run build:ecm && npm run build:commonjs",
    "build:ecm": "tsc -p tsconfig.prod.json",
    "build:commonjs": "tsc -p tsconfig.commonjs.json",
    "lint": "eslint '**/*.ts'"
  },
  "author": "Alasdair McLeay",
  "license": "ISC",
  "keywords": [
    "react",
    "react-native",
    "react-native-web",
    "accessibility",
    "a11y"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/penx/react-native-accessibility-props.git"
  },
  "bugs": {
    "url": "https://github.com/penx/react-native-accessibility-props/issues"
  },
  "homepage": "https://github.com/penx/react-native-accessibility-props#readme",
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^27.4.1",
    "@types/react-native": "^0.70.4",
    "eslint": "^8.13.0",
    "eslint-config-react-app": "^7.0.0",
    "jest": "^27.5.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-native": "^0.70.2",
    "react-test-renderer": "^18.1.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.9.1",
    "tsd": "^0.24.1",
    "typescript": "^4.8.4"
  }
}
